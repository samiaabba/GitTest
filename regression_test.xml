<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Adhoc">
	<listeners>
		<listener class-name="account.gmail.com.ScreenshotListener"></listener>
	</listeners>
	<test thread-count="5" name="Chrome_Test">
		<parameter name="url" value="https://www.google.com"></parameter>
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="run.Execute" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->